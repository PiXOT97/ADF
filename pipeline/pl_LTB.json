{
	"name": "pl_LTB",
	"properties": {
		"activities": [
			{
				"name": "ChecForFiles",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "ds_main_BinaryPath",
						"type": "DatasetReference",
						"parameters": {
							"Container": {
								"value": "@pipeline().parameters.path_container",
								"type": "Expression"
							},
							"FilePath": {
								"value": "@pipeline().parameters.path_lz_input",
								"type": "Expression"
							}
						}
					},
					"fieldList": [],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "BinaryReadSettings"
					}
				}
			}
		],
		"parameters": {
			"path_container": {
				"type": "String",
				"defaultValue": "vendor"
			},
			"path_lz_input": {
				"type": "String",
				"defaultValue": "landing_zone/input"
			},
			"path_lz_errors": {
				"type": "String",
				"defaultValue": "landing_zone/errors"
			},
			"path_lz_processed": {
				"type": "String",
				"defaultValue": "landing_zone/processed"
			},
			"path_b_input": {
				"type": "String",
				"defaultValue": "bronze/input"
			},
			"path_b_errors": {
				"type": "String",
				"defaultValue": "bronze/errors"
			},
			"path_b_processed": {
				"type": "String",
				"defaultValue": "bronze/processed"
			}
		},
		"folder": {
			"name": "pl_PROFI/pl_LTB"
		},
		"annotations": []
	}
}